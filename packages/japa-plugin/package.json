  "peerDependencies": {
    "@japa/runner": "^2.0.0"
  },
  "dependencies": {
    "@julr/factorio": "link:../core"
  },
  "devDependencies": {
    "@japa/runner": "^2.2.1",
    "knex": "^2.3.0",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11"
