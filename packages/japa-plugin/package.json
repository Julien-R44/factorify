{
  "name": "@julr/japa-factorio-plugin",
  "version": "1.0.0-beta.0",
  "description": "The Japa plugin for Factorio",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "ISC",
  "keywords": [
    "japa",
    "japa plugin",
    "knex",
    "factorio",
    "model factory"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
